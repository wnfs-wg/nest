{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "noUnusedLocals": false
  },
  "include": ["src", "test"],
  "exclude": ["node_modules", "dist", "out"],
  "typedocOptions": {
    "entryPoints": ["src/index.js", "src/module1.js"],
    "includeVersion": true,
    "excludeExternals": true,
    "internalModule": "<internal>"
  }
}
